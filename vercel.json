{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "functions": {
    "api/**": { "runtime": "@vercel/node@5.3.21" }
  },
  "rewrites": [
    { "source": "/api/quizzes/:path*", "destination": "/api/quizzes/[[...path]]" },
    { "source": "/api/playtime/:path*", "destination": "/api/playtime/[[...path]]" },
    { "source": "/api/parent/:path*", "destination": "/api/parent/[[...path]]" },
    { "source": "/api/profile/:path*", "destination": "/api/index" },
    { "source": "/api/:path*", "destination": "/api/index" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}